{
    "name": "tabby-mingze-tearoff",
    "version": "0.1.0",
    "description": "Tabby plugin that detaches tabs into standalone windows",
    "keywords": [
        "tabby-plugin"
    ],
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "scripts": {
        "build": "webpack --progress --color --mode production",
        "watch": "webpack --progress --color --watch --mode development",
        "prepublishOnly": "npm run build",
        "build:dev": "webpack --progress --color --mode development",
        "typecheck": "tsc --noEmit"
    },
    "files": [
        "dist"
    ],
    "author": "mingze",
    "license": "MIT",
    "devDependencies": {
        "@angular/animations": "^12.0.0",
        "@angular/common": "^12.0.0",
        "@angular/core": "^12.0.0",
        "@angular/forms": "^12.0.0",
        "@angular/platform-browser": "^12.0.0",
        "@ng-bootstrap/ng-bootstrap": "^2.2.0",
        "@types/node": "14.18.63",
        "@types/webpack-env": "^1.16.0",
        "apply-loader": "^2.0.0",
        "css-loader": "^5.1.1",
        "pug": "^2.0.3",
        "pug-loader": "^2.4.0",
        "rxjs": "^7.3.0",
        "sass": "^1.77.8",
        "sass-loader": "^11.0.1",
        "style-loader": "^2.0.0",
        "tabby-core": "1.0.163",
        "tabby-settings": "1.0.163",
        "tabby-terminal": "1.0.163",
        "ts-loader": "^5.2.1",
        "typescript": "4.2.4",
        "webpack": "^5.24.4",
        "webpack-cli": "^4.5.0"
    }
}
